{
	"name": "@printf83/bsts-test",
	"version": "0.1.71",
	"description": "Bootstrap TS builder - Documentation",
	"keywords": [
		"documentation",
		"bootstrap 5",
		"api",
		"sdk",
		"typescript"
	],
	"author": "Hamzah A <printf83@gmail.com>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/printf83/bsts-test/issues"
	},
	"homepage": "https://github.com/printf83/bsts-test#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/printf83/bsts-test.git"
	},
	"main": "./lib/index.js",
	"types": "./lib/types/index.d.ts",
	"files": [
		"lib/**/*"
	],
	"scripts": {
		"clean": "rm -rf ./lib",
		"clean:win": "rmdir /s lib",
		"build": "npm run clean && npm run build:esm",
		"build:win": "npm run clean:win && npm run build:esm",
		"build:esm": "tsc -p ./configs/tsconfig.esm.json",
		"debug": "tsc -p ./configs/tsconfig.esm.json --watch",
		"link": "sudo npm link ../bsts",
		"unlink": "sudo npm unlink ../bsts",
		"debug:link": "npm run unlink && npm run link && npm run debug",
		"webpack:debug": "webpack -w",
		"webpack": "npm run build && webpack",
		"prepack": "npm run build"
	},
	"devDependencies": {
		"@printf83/bsts": "^0.1.71",
		"@types/bootstrap": "^5.2.6",
		"@types/js-beautify": "^1.13.3",
		"ts-loader": "^9.4.2",
		"typescript": "^5.0.3",
		"webpack": "^5.77.0",
		"webpack-cli": "^5.0.1"
	}
}
